    <h2 class="main-section__title"><%= title %></h2>
    <h3 class="main-section__subtitle"><%= subtitle %></h3>
  </section>
  <% if(users && users.length > 0) { %>
  <%- include('partials/users') %>
  <% } else { %>
  <section>
    <h3>Ops... não encontramos nenhum usuário no banco de dados</h3>
  </section>
  <% } %>
